{
  "name": "@runonbitcoin/tokenkit",
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/tokenkit.cjs.js",
      "import": "./dist/tokenkit.esm.js"
    }
  },
  "browser": "dist/tokenkit.min.js",
  "unpkg": "dist/tokenkit.min.js",
  "types": "src/index.d.ts",
  "packageManager": "yarn@3.2.0",
  "scripts": {
    "build": "run-s clean && node build.js",
    "clean": "rm -rf dist",
    "test": "mocha \"test/**/*.test.js\"",
    "test:cjs": "run-s build && env LIB=dist/tokenkit.cjs mocha \"test/**/*.test.js\"",
    "test:mjs": "run-s build && env LIB=dist/tokenkit.mjs mocha \"test/**/*.test.js\"",
    "test:browser": "node test/env/browser.js",
    "test:types": "tsd"
  },
  "dependencies": {
    "@runonbitcoin/nimble": "^1.0.13",
    "ky": "^0.30.0",
    "ky-universal": "^0.10.1"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "esbuild": "^0.14.34",
    "esbuild-plugin-globals": "^0.1.1",
    "esm": "^3.2.25",
    "glob": "^8.0.1",
    "mocha": "^9.2.2",
    "npm-run-all": "^4.1.5",
    "run-sdk": "^0.6.37",
    "selenium-webdriver": "^4.1.2",
    "tsd": "^0.20.0"
  },
  "peerDependencies": {
    "run-sdk": "*"
  },
  "tsd": {
    "directory": "test"
  }
}
